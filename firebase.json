{
  "hosting": [
    {
      "target": "leadzilla-landing-page",
      "public": "out",
      "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
      "cleanUrls": true,
      "redirects": [ {
        "source": "/blog",
        "destination": "https://blog.leadzilla.ai/",
        "type": 301
      } ],
      "rewrites": [
        {
          "source": "**",
          "destination": "/index.html"
        }
      ]
    }
  ]
}
